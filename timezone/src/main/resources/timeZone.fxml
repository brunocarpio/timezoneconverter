<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.Region?>

<fx:root type="javafx.scene.layout.HBox" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17.0.2-ea" alignment="CENTER_LEFT" prefHeight="37.0" prefWidth="496.0">
    <children>
        <VBox prefHeight="61.0" prefWidth="143.0">
            <children>
                <Label fx:id="regionLabel" text="Zone1" textFill="WHITE">
                    <font>
                        <Font name="Corbel Bold" size="18.0" />
                    </font>
                </Label>
                <Label fx:id="countryLabel" text="Zone2" textFill="WHITE">
                    <font>
                        <Font name="Corbel Bold" size="16.0" />
                    </font>
                </Label>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </VBox>
        <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
        <Label fx:id="timeLabel" text="Time" textFill="WHITE">
            <font>
                <Font name="Corbel Bold" size="24.0" />
            </font>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </Label>
    </children>
</fx:root>